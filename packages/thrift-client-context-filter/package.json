        "test": "npm run test:only",
        "test:only": "echo 'No tests for thrift-client-context-filter'; exit 0;"
    },
    "peerDependencies": {
        "@creditkarma/thrift-client": "^1.0.0",
        "@creditkarma/thrift-server-core": "^1.0.0"
    },
    "publishConfig": {
