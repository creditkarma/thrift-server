{
  "name": "@creditkarma/thrift-client",
  "version": "0.7.0-3",
  "description": "Thrift client library for NodeJS written in TypeScript.",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "files": [
    "dist/main"
  ],
  "directories": {
    "example": "example"
  },
  "keywords": [
    "thrift",
    "typescript",
    "rpc",
    "microservices",
    "http client",
    "framework"
  ],
  "scripts": {
    "clean": "rimraf ./dist ./src/tests/integration/generated",
    "lint": "tslint --fix './src/**/*.ts'",
    "codegen": "thrift-typescript --target thrift-server --rootDir ./src/tests/integration --sourceDir ./thrift --outDir ./generated",
    "prebuild": "npm run clean",
    "build": "npm run lint && tsc -p tsconfig.json",
    "prebuild:test": "npm run clean && npm run codegen",
    "build:test": "tsc -p tsconfig.test.json",
    "watch": "tsc --watch",
    "start": "ts-node --project ./example ./example/index.ts",
    "pretest": "npm run build:test",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "lab --timeout 10000 --verbose -l -S -P spec dist/tests/unit",
    "test:integration": "lab --timeout 10000 --verbose -l -S -P spec dist/tests/integration"
  },
  "author": "Credit Karma",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/creditkarma/thrift-server/tree/master/packages/thrift-client"
  },
  "peerDependencies": {
    "@creditkarma/thrift-server-core": "next",
    "@types/request": "^2.47.0",
    "request": "^2.83.0"
  },
  "devDependencies": {
    "@creditkarma/dynamic-config": "~0.6.7",
    "@creditkarma/thrift-server-core": "^0.7.0-3",
    "@creditkarma/thrift-server-express": "^0.7.0-3",
    "@creditkarma/thrift-server-hapi": "^0.7.0-3",
    "@creditkarma/thrift-typescript": "2.0.0-11",
    "@types/body-parser": "^1.16.5",
    "@types/code": "^4.0.3",
    "@types/express": "^4.0.37",
    "@types/hapi": "^16.1.10",
    "@types/lab": "^11.1.0",
    "@types/node": "^8.0.31",
    "@types/q": "^1.0.5",
    "@types/request": "^2.47.0",
    "body-parser": "^1.17.2",
    "code": "^4.1.0",
    "express": "^4.16.0",
    "hapi": "^16.6.2",
    "lab": "^14.3.1",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "2.8.x"
  },
  "dependencies": {
    "async-hook": "^1.7.1",
    "zipkin": "^0.12.0"
  }
}
