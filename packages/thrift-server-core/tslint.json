{
	"extends": "tslint:recommended",
	"rules": {
    "no-empty": false,
    "no-bitwise": false,
		"object-literal-sort-keys": false,
		"no-console": false,
    "no-var-requires": false,
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ]
	}
}
