{
  "name": "@creditkarma/vault-client",
  "version": "0.0.0",
  "description": "A client for communicating with Hashicorp Vault written in TypeScript",
  "main": "dist/main/index.js",
  "types": "dist/main/index.d.ts",
  "files": [
    "dist/main"
  ],
  "keywords": [
    "TypeScript",
    "Hashicorp",
    "Vault"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "lint": "tslint --fix src/**/*.ts",
    "build": "npm run clean && npm run lint && tsc",
    "docker": "docker-compose up --force-recreate vault &",
    "docker:kill": "docker-compose kill",
    "pretest": "npm run build && npm run docker",
    "posttest": "npm run docker:kill",
    "test": "wait-on --timeout 10000 http://localhost:8201 && npm run lint && npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test mocha --full-trace dist/tests/unit/**/*.spec.js",
    "test:integration": "NODE_ENV=test mocha --full-trace --timeout 15000 dist/tests/integration/**/*.spec.js"
  },
  "author": "Credit Karma",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chai": "^4.0.5",
    "@types/debug": "0.0.30",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.51",
    "chai": "^4.1.2",
    "mocha": "^4.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "wait-on": "^2.0.2"
  },
  "dependencies": {
    "@types/request": "^2.0.8",
    "@types/request-promise-native": "^1.0.10",
    "debug": "^3.1.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5"
  }
}
